{
    "$schema": "https://raw.githubusercontent.com/zheka2304/innercore-mod-toolchain/develop/toolchain/schemas/make.schema.json",
	"resources": [
		{
			"path": "assets",
			"type": "resource_directory"
		},
		{
			"path": "structures",
			"type": "resource_directory"
		},
		{
			"path": "assets/gui",
			"type": "gui"
		}
	], 
	"sources": [
		{
			"source": "code",
			"target": "main.js",
			"type": "main",
			"language": "typescript"
		},
		{
			"source": "launcher.js",
			"type": "launcher",
			"language": "javascript"
		},
        {
            "source": "dev/Component",
            "type": "library",
            "language": "typescript",
            "target": "../../release/Component/Component.js"
        }
	],
	"additional": [
		{
			"source": "config.json",
			"targetDir": "."
		},
		{
			"source": "config.info.json",
			"targetDir": "."
		}
	],
	"info": {
		"name": "libraries",
		"author": "Artem Kot",
		"version": "1",
		"description": "Библиотеки автора проекта"
	},
	"api": "CoreEngine",
	"linkNative": [
		"minecraftpe",
		"innercore",
		"nativejs"
	],
	"excludeFromRelease": [],
    "tsconfig": {
        "jsx": "react",
		"jsxFactory": "UI.parseElement",
		"jsxFragmentFactory": "UI.Fragment",
		"moduleResolution": "nodenext"
    }
}




















